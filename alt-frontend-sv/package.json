{
	"name": "alt-frontend-sv",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:mock": "concurrently -k -n mock,vite \"bun run mock-server\" \"bun run dev --host\"",
		"mock-server": "bunx tsx tests/e2e/infra/mock-server.ts",
		"build": "vite build",
		"preview": "vite preview --host 127.0.0.1 --port 4173 --strictPort",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome lint --write",
		"format": "biome format --write",
		"test": "vitest run",
		"test:server": "vitest run --project=server",
		"test:client": "VITEST_BROWSER=true vitest run --project=client",
		"test:all": "VITEST_BROWSER=true vitest run",
		"test:watch": "vitest --watch",
		"test:e2e": "bun run build && npx playwright test --reporter=list",
		"test:e2e:ui": "npx playwright test --ui",
		"build:analyze": "vite build && echo 'Bundle report: stats.html'"
	},
	"dependencies": {
		"@bufbuild/protobuf": "^2.11.0",
		"@connectrpc/connect": "^2.1.1",
		"@connectrpc/connect-web": "^2.1.1",
		"@ory/client": "^1.22.25",
		"@tanstack/svelte-query": "^6.0.18",
		"bits-ui": "^2.16.1",
		"chart.js": "^4.5.1",
		"clsx": "^2.1.1",
		"isomorphic-dompurify": "^2.36.0",
		"svelte-chartjs": "^3.1.5",
		"tailwind-merge": "^3.5.0",
		"tailwind-variants": "^3.2.2",
		"valibot": "^1.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.3",
		"@bufbuild/protoc-gen-es": "^2.11.0",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.575.0",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/adapter-node": "^5.5.3",
		"@sveltejs/enhanced-img": "^0.10.2",
		"@sveltejs/kit": "^2.52.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.2.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.3.0",
		"@vitest/browser": "^4.0.18",
		"@vitest/browser-playwright": "^4.0.18",
		"axe-core": "^4.11.1",
		"axe-playwright": "^2.2.2",
		"concurrently": "^9.2.1",
		"jsdom": "^28.1.0",
		"jsonwebtoken": "^9.0.3",
		"msw": "^2.12.10",
		"rollup-plugin-visualizer": "^7.0.0",
		"svelte": "^5.53.0",
		"svelte-check": "^4.4.2",
		"tailwindcss": "^4.2.0",
		"tsx": "^4.21.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2"
	}
}
