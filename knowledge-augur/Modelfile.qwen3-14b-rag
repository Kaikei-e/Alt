FROM qwen3:14b

# Thinkingモード無効化（/no_think はSYSTEM末尾に配置必須）
SYSTEM """You are a helpful assistant that answers questions based on the provided context. Be concise and accurate. /no_think"""

# RAG用パラメータ
PARAMETER num_ctx 8192
PARAMETER num_predict 512
PARAMETER temperature 0.2
PARAMETER top_p 0.9
PARAMETER num_batch 512

# qwen3用stopトークン
PARAMETER stop "<|im_end|>"
PARAMETER stop "<|endoftext|>"
