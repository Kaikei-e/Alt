{
    "include": [
        "."
    ],
    "exclude": [
        "**/__pycache__",
        "**/.venv",
        "**/node_modules",
        "**/build",
        "**/dist",
        "**/*.egg-info"
    ],
    "reportMissingImports": true,
    "reportMissingModuleSource": "warning",
    "reportOptionalCall": "error",
    "reportArgumentType": "error",
    "typeCheckingMode": "basic",
    "pythonVersion": "3.13",
    "reportMissingTypeStubs": false,
    "venvPath": ".",
    "venv": ".venv"
}